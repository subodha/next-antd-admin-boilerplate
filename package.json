{
  "name": "next-antd-admin-boilerplate",
  "version": "0.1.0",
  "private": true,
  "author": "Subodha Pathiraja",
  "license": "MIT",
  "scripts": {
    "-------------------------------- LINT ----": "----",
    "lint": "next lint",
    "lintall": "tsc --noEmit && eslint src/**/*.ts{,x}",
    "fix": "yarn lint --fix",
    "tsc-check": "tsc -v && tsc --noEmit",
    "format": "prettier --write src/**/*.ts{,x}",
    "prepare": "husky install",
    "-------------------------------- DEV  ----": "----",
    "dev": "next dev",
    "start": "next start",
    "serve": "yarn build && yarn start",
    "-------------------------------- PROD ----": "----",
    "build": "next build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-commit": "git update-index --again"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,tsx,css,less,json}": [
      "yarn format",
      "git add"
    ]
  },
  "dependencies": {
    "add": "^2.0.6",
    "antd": "^4.16.13",
    "autoprefixer": "^10.3.4",
    "classnames": "^2.3.1",
    "css-loader": "^6.3.0",
    "next": "11.1.2",
    "next-plugin-antd-less": "^1.4.3",
    "normalize.css": "^8.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "yarn": "^1.22.11"
  },
  "devDependencies": {
    "@types/react": "17.0.21",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "babel-plugin-import": "^1.13.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.0",
    "eslint-config-next": "11.1.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-qcolate": "^1.2.7",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.25.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "stylelint": "^13.13.1",
    "stylelint-config-standard": "^22.0.0",
    "typescript": "4.4.3"
  }
}
